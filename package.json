{
  "name": "@tensorrent/sdk",
  "version": "1.2.0",
  "description": "Compute core leasing protocol with HTTP 402 micropayments and 403 identity verification",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "bin": {
    "tensorrent": "./bin/tensorrent.js"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "compute",
    "leasing",
    "http-402",
    "micropayments",
    "http-403",
    "identity",
    "solana",
    "blockchain",
    "gpu",
    "cpu",
    "distributed"
  ],
  "author": "TensorRent",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/HudneyGomesNunes/TensorRent"
  },
  "bugs": {
    "url": "https://github.com/HudneyGomesNunes/TensorRent/issues"
  },
  "homepage": "https://github.com/HudneyGomesNunes/TensorRent#readme",
  "dependencies": {
    "@solana/web3.js": "^1.87.6",
    "axios": "^1.6.2",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/ws": "^8.5.10",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.0.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist",
    "bin",
    "LICENSE",
    "CHANGELOG.md"
  ]
}
